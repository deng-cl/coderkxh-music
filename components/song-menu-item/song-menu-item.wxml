<wxs src="/utils/formates.wxs" module="formates"/>
<view class="item" style="width: {{ width === 0 ? 236 : width}}rpx;" bind:tap="onSongMenuTap">
    <view class="top">
        <image  class="image" src="{{ itemData.coverImgUrl }}" mode="widthFix"/>
        <view class="count">{{ formates.formatCount(itemData.playCount) }}</view>
    </view>
    <view class="bottom">{{ itemData.name }}</view>
</view>